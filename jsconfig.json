// this config follow vetur https: //vuejs.github.io/vetur/guide/
{
  "compilerOptions": {
    // "module": "esnext",
    // "target": "es2015",
    "module": "commonjs",
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.vue", "src/**/*.js", "src/**/*.ts"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
